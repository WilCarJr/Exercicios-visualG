Algoritmo "CONSUMO"

Var
// Seção de Declarações das variáveis 
G, M: Real
D: Inteiro

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
      Escreva ("Distancia percorrida: ")
      Leia (D)
      
     Escreva ("Combustível gasto: ")
     Leia (G)

     M <- D / G
     
     Escreval ("Consumo medio = ", (M):5:3)
      

Fimalgoritmo