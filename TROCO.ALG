Algoritmo "CIRCULO"

Var
// Seção de Declarações das variáveis 
A, R: Real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
      Escreva ("Digite o valor do raio do circulo: ")
      Leia (R)
      
      A <- 3.14159 * R ^ 2
     
     Escreval ("AREA = ", (A):4:3)
      

Fimalgoritmo